{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "apy/breadcrumbtrail-bundle": "dev-master",
        "arkounay/ux-collection": "^2.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "easycorp/easyadmin-bundle": "^4.3",
        "friendsofsymfony/jsrouting-bundle": "^3.2",
        "gesdinet/jwt-refresh-token-bundle": "^1.1",
        "hautelook/alice-bundle": "^2.11",
        "knplabs/knp-paginator-bundle": "^5.9",
        "leogout/seo-bundle": "^1.3",
        "liip/imagine-bundle": "^2.8",
        "nelmio/cors-bundle": "^2.2",
        "nesbot/carbon": "^2.62",
        "php-http/curl-client": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.8",
        "sensio/framework-extra-bundle": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/doctrine-messenger": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/expression-language": "6.0.*",
        "symfony/flex": "^2",
        "symfony/form": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-client": "6.0.*",
        "symfony/intl": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/mercure-bundle": "^0.3.5",
        "symfony/mercure-notifier": "6.0.*",
        "symfony/mime": "6.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.0.*",
        "symfony/process": "6.0.*",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/string": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/ux-autocomplete": "^2.4",
        "symfony/ux-cropperjs": "^2.4",
        "symfony/ux-dropzone": "^2.4",
        "symfony/ux-lazy-image": "^2.4",
        "symfony/ux-live-component": "2.x-dev",
        "symfony/ux-react": "^2.4",
        "symfony/ux-turbo": "^2.4",
        "symfony/ux-turbo-mercure": "^2.4",
        "symfony/ux-twig-component": "^2.4",
        "symfony/validator": "6.0.*",
        "symfony/web-link": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.15",
        "symfony/yaml": "6.0.*",
        "symfonycasts/reset-password-bundle": "^1.14",
        "symfonycasts/verify-email-bundle": "^1.11",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^3.4",
        "twig/inky-extra": "^3.4",
        "twig/intl-extra": "^3.4",
        "twig/string-extra": "^3.4",
        "twig/twig": "^2.12|^3.0",
        "typesense/typesense-php": "^4.8",
        "vich/uploader-bundle": "^1.21"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "KMS\\FroalaEditorBundle\\": "lib/froala/KMSFroalaEditorBundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "php bin/console doctrine:schema:update --force"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.0.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "mbezhanov/faker-provider-collection": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/maker-bundle": "1.44",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "6.0.*",
        "symfony/web-profiler-bundle": "6.0.*",
        "zenstruck/foundry": "^1.21"
    }
}
